{
  "name": "@serenity-js/serenity-js-cucumber-protractor-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "serenity-bdd update",
    "clean": "rimraf target",
    "test": "failsafe clean test:execute test:report",
    "test:execute": "protractor ./protractor.conf.js",
    "test:wip": "failsafe clean test:execute:wip test:report",
    "test:wip:execute": "protractor ./protractor.conf.js --cucumberOpts.tags=@wip",
    "test:report": "serenity-bdd run --features ./features",
    "lint": "eslint --ext ts --config .eslintrc.js .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serenity-js/serenity-js-cucumber-protractor-template.git"
  },
  "keywords": [
    "serenity-js",
    "bdd",
    "cucumber",
    "protractor",
    "testing"
  ],
  "author": "Jan Molak <jan.molak@serenity.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/serenity-js/serenity-js-cucumber-protractor-template/issues"
  },
  "homepage": "http://serenity-js.org",
  "engines": {
    "node": "^12 || ^14 || ^16",
    "npm": "^6 || ^7 || ^8.0.0"
  },
  "dependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@serenity-js/assertions": "^2.32.7",
    "@serenity-js/console-reporter": "^2.32.7",
    "@serenity-js/core": "^2.32.7",
    "@serenity-js/cucumber": "^2.32.7",
    "@serenity-js/protractor": "^2.32.7",
    "@serenity-js/serenity-bdd": "^2.32.7",
    "@types/node": "^16.11.21",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chromedriver": "^97.0.2",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "is-ci": "^3.0.1",
    "npm-failsafe": "^0.4.3",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.4.4"
  }
}
